        <script src="https://cdnjs.cloudflare.com/ajax/libs/dropzone/5.7.2/dropzone.min.js"></script>
<script>
    Dropzone.options.myDropzone = {
        url: "{{ route('upload-property-image') }}",
        maxFilesize: 2, // Taille maximale du fichier en MB
        acceptedFiles: 'image/*',
        addRemoveLinks: true,
        success: function (file, response) {
            console.log('File uploaded successfully');
        },
        error: function (file, response) {
            console.log('File upload error: ' + response);
        }
    };

    $('#form1').submit(function (e) {
        e.preventDefault(); // Empêcher la soumission par défaut du formulaire

        var formData = new FormData(this);

        $.ajax({
            type: 'POST',
            url: "{{ route('add-property-post') }}",
            data: formData,
            processData: false,
            contentType: false,
            success: function (response) {
                alert('Propriété ajoutée avec succès.');
                window.location.href = "{{ route('add-property-form') }}";
            },
            error: function (response) {
                alert('Erreur lors de l\'ajout de la propriété.');
                console.log(response);
            }
        });
    });
</script>
